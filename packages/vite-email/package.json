{
  "name": "vite-email",
  "version": "0.7.4",
  "description": "Send emails rendered by Vite and Markdown-It automatically",
  "keywords": [
    "vite",
    "vite-plugin",
    "markdown",
    "email"
  ],
  "homepage": "https://github.com/yjl9903/vite-email#readme",
  "bugs": {
    "url": "https://github.com/yjl9903/vite-email/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjl9903/vite-email.git"
  },
  "license": "MIT",
  "author": "XLor",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "vmail": "./vmail.mjs"
  },
  "files": [
    "dist",
    "build",
    "*.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "format": "prettier --write src/**/*.ts",
    "start": "node vmail.mjs",
    "test": "vitest",
    "test:ci": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@breadc/color": "^0.8.5",
    "@breadc/complete": "^0.8.5",
    "@breadc/progress": "^0.8.5",
    "breadc": "^0.8.5",
    "cli-progress": "^3.11.2",
    "csv-parse": "^5.3.4",
    "csv-stringify": "^6.2.3",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-title": "^4.0.0",
    "nodemailer": "^6.9.1",
    "prompts": "^2.4.2",
    "sirv": "^2.0.2",
    "ufo": "^1.0.1",
    "vite": "^4.1.1",
    "vite-plugin-singlefile": "0.13.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.19",
    "@types/nodemailer": "^6.4.7",
    "@types/prompts": "^2.4.0",
    "vitest": "0.28.4"
  }
}
